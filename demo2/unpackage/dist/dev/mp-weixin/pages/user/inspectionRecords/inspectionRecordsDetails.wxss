
.contentTop {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		padding: 40rpx;
		height: 150rpx;
		border-bottom: 1rpx solid #CCCCCC;
}
.contentTopImg {
		margin-right: 80rpx;
}
.contentTopImg image {
		width: 150rpx;
		height: 150rpx;
		border-radius: 16rpx;
		-webkit-box-shadow: 15rpx 15rpx 5rpx #CCCCCC;
		        box-shadow: 15rpx 15rpx 5rpx #CCCCCC;
}
.contentTopInfo {
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-orient: vertical;
		-webkit-box-direction: normal;
		-webkit-flex-direction: column;
		    -ms-flex-direction: column;
		        flex-direction: column;
}
.contentTopInfo text {
		padding: 0 20rpx;
		font-weight: 500;
}
.name,
	.sex,
	.age {
		font-weight: bold;
		height: 33%;
}
.title{
	color: #666666;
	font-size: 24rpx;
}
.title,.list,.result {
		padding: 10rpx 40rpx;
}
.title,.list {
		border-bottom: 1rpx solid #CCCCCC;
}
.list .li{
		color: #999999;
		font-size: 26rpx;
}
.list .if{
		color: #FF9999;
		font-size: 26rpx;
}
.listTitle{
		padding: 0 20rpx;
}
.listTitle text{
		display: inline-block;
		width: 20rpx;
		height: 20rpx;
		border-radius: 50%;
		background-color: #3CC51F;
		margin-right: 10rpx;
}
.result{
		font-size: 24rpx;
		color: #009966;
}
.resultTitle{
		color: #0033B2;
		font-size: 26rpx;
		font-weight: bold;
}

