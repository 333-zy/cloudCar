
.search{
	position: relative;
	border: 20rpx solid #EFEFF4;
}
.search input{
	height: 50rpx;
	line-height: 50rpx;
	min-height: 50rpx;
}
.search text{
	position: absolute;
	right: 0;
	top: -10rpx;
	padding: 0 10rpx;
}
.text{
	text-align: center;
	height: 50rpx;
	line-height: 50rpx;
	color: #808080;
	font-size: 28rpx;
}
.list{
	padding: 0 20rpx;
	border-top: 1rpx solid #F2F2F2;
}
.list-top{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
}
.name{
	width: 130rpx;
	color: #3BCEBE;
}
.sex,.day{
	padding: 0 10rpx;
}
.list-bottom{
	height: 100rpx;
	line-height: 50rpx;
	color: #999999;
}
.time{
	color: #999999;
	text-align: right;
	font-size: 22rpx;
}

